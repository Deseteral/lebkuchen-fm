{
  "editor.rulers": [
    120
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "javascript.validate.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    ".proxyrc": "json"
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#302F01",
    "titleBar.activeBackground": "#444201",
    "titleBar.activeForeground": "#FEFDC5"
  }
}
