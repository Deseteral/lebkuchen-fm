{
  "extends": [
    "airbnb",
    "react-app"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".jsx"] }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
