{
  "name": "lebkuchen-fm",
  "version": "2.0.0",
  "main": "src/main.js",
  "repository": "https://github.com/Deseteral/lebkuchen-fm.git",
  "author": "Deseteral <deseteral@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/main.js",
    "build": "babel src -d build --extensions '.ts,.js'",
    "dev": "npm run build && npm run start",
    "test": "npm run test:type-check",
    "test:type-check": "tsc --noEmit",
    "test:type-check:watch": "npm run test:type-check -- --watch"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-ws": "4.0.0",
    "shortid": "2.2.15",
    "signale": "1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@deseteral/eslint-config": "2.1.0",
    "@types/express": "4.17.6",
    "@types/express-ws": "3.0.0",
    "@types/shortid": "0.0.29",
    "@types/signale": "1.4.1",
    "eslint": "6.8.0",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "ws": "*"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/typescript"
    ]
  },
  "eslintConfig": {
    "extends": "@deseteral/eslint-config/typescript"
  }
}
