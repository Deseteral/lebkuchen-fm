{
  "name": "lebkuchen-fm",
  "version": "1.0.0",
  "description": "YouTube TV like service with super powers controlled by chatbot",
  "scripts": {
    "start": "node build/Main.js",
    "build": "npm run server:build && npm run frontend:build",
    "lint": "npm run lint:server && npm run lint:frontend",
    "test": "exit 0 # npm run lint",
    "frontend:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "frontend:build": "cross-env NODE_ENV=production webpack",
    "server:build": "gulp",
    "server:dev": "npm run server:build",
    "lint:server": "eslint server --ext ts,tsx",
    "lint:frontend": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "express": "4.16.4",
    "mongodb": "3.1.13",
    "node-fetch": "2.3.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "youtube-player": "5.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "4.16.1",
    "@types/mongodb": "3.1.19",
    "@types/node": "11.9.3",
    "@types/node-fetch": "2.1.6",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.1",
    "@types/socket.io": "2.1.2",
    "@types/socket.io-client": "1.4.32",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "@typescript-eslint/parser": "1.3.0",
    "babel-loader": "8.0.5",
    "cross-env": "5.2.0",
    "del": "3.0.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "typescript": "3.3.3",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  },
  "heroku-run-build-script": true
}
