{
  "name": "lebkuchen-fm-client",
  "version": "2.1.0",
  "description": "Lebkuchen FM client application",
  "repository": "https://github.com/Deseteral/lebkuchen-fm.git",
  "author": "Deseteral <deseteral@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "build": "parcel build src/index.html",
    "dev": "parcel src/index.html",
    "start": "npm run dev",
    "clean": "rm -rf dist",
    "test": "npm run test:type-check && npm run test:lint",
    "test:type-check": "tsc --noEmit",
    "test:type-check:watch": "npm run test:type-check -- --watch",
    "test:lint": "eslint src --ext js,jsx,ts,tsx"
  },
  "dependencies": {
    "lebkuchen-fm-service": "*",
    "mitt": "2.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.2",
    "socket.io-client": "4.2.0",
    "styled-components": "5.2.0",
    "yt-player": "3.4.3"
  },
  "devDependencies": {
    "@deseteral/eslint-config": "6.0.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "@types/styled-components": "5.1.24",
    "@types/yt-player": "3.4.2",
    "buffer": "6.0.3",
    "cssnano": "5.0.8",
    "eslint": "8.15.0",
    "events": "3.3.0",
    "parcel": "2.5.0",
    "postcss": "8.3.8",
    "postcss-import": "14.0.2",
    "process": "0.11.10",
    "tailwindcss": "2.2.16",
    "typescript": "4.5.4"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "eslintConfig": {
    "extends": "@deseteral/eslint-config/typescript",
    "rules": {
      "max-len": "off",
      "no-console": "off"
    }
  }
}
