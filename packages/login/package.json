{
  "name": "lebkuchen-fm-login",
  "version": "2.1.0",
  "description": "Lebkuchen FM auth frontend",
  "repository": "https://github.com/Deseteral/lebkuchen-fm.git",
  "author": "Deseteral <deseteral@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "parcel build src/login.html",
    "dev": "parcel src/login.html",
    "start": "npm run dev",
    "clean": "rm -rf dist",
    "test": "npm run test:type-check && npm run test:lint",
    "test:type-check": "tsc --noEmit",
    "test:type-check:watch": "npm run test:type-check -- --watch",
    "test:lint": "eslint src --ext js,jsx,ts,tsx"
  },
  "devDependencies": {
    "@deseteral/eslint-config": "5.0.0",
    "autoprefixer": "10.4.2",
    "cssnano": "5.1.4",
    "eslint": "7.32.0",
    "parcel": "2.3.2",
    "postcss": "8.4.8",
    "postcss-import": "14.0.2",
    "tailwindcss": "2.2.16",
    "typescript": "4.5.4"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "eslintConfig": {
    "extends": "@deseteral/eslint-config/typescript",
    "rules": {
      "max-len": "off",
      "no-console": "off"
    }
  }
}
