{
  "name": "lebkuchen-fm-service",
  "version": "2.0.0",
  "description": "LebkuchenFM backend service",
  "main": "src/lib.ts",
  "repository": "https://github.com/Deseteral/lebkuchen-fm.git",
  "author": "Deseteral <deseteral@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node build/main.js",
    "build": "babel src -d build --extensions '.ts,.js' --copy-files --source-maps",
    "clean": "rm -rf build",
    "dev": "npm run build && npm run start",
    "test": "npm run test:type-check && npm run test:lint && npm run test:unit",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:type-check": "tsc --noEmit",
    "test:type-check:watch": "npm run test:type-check -- --watch",
    "test:lint": "eslint src --ext js,jsx,ts,tsx"
  },
  "dependencies": {
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "dropbox": "10.12.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "glob": "7.2.0",
    "http-status-codes": "2.1.4",
    "memorystore": "1.6.7",
    "mitt": "2.1.0",
    "mongodb": "4.6.0",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.9.0",
    "socket.io": "4.2.0",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.0",
    "@babel/plugin-proposal-decorators": "7.17.12",
    "@babel/preset-env": "7.18.0",
    "@babel/preset-typescript": "7.17.12",
    "@deseteral/eslint-config": "6.0.1",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.8",
    "@types/express-session": "1.17.4",
    "@types/mongodb": "3.5.27",
    "@types/glob": "7.2.0",
    "@types/jest": "27.5.1",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "8.15.0",
    "jest": "28.1.0",
    "typescript": "4.6.4"
  }
}
