{
  "name": "lebkuchen-fm-service",
  "description": "LebkuchenFM backend service",
  "version": "2.0.0",
  "main": "src/main.js",
  "repository": "https://github.com/Deseteral/lebkuchen-fm.git",
  "author": "Deseteral <deseteral@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src -d build --extensions '.ts,.js' --copy-files",
    "dev": "npm run build && npm run start",
    "test": "npm run test:type-check",
    "test:type-check": "tsc --noEmit",
    "test:type-check:watch": "npm run test:type-check -- --watch"
  },
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.17.1",
    "pino": "6.2.1",
    "pino-multi-stream": "5.0.0",
    "pino-pretty": "4.0.0",
    "socket.io": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@deseteral/eslint-config": "2.1.0",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.6",
    "@types/pino": "6.0.0",
    "@types/pino-multi-stream": "3.1.2",
    "@types/socket.io": "2.1.4",
    "eslint": "6.8.0",
    "typescript": "3.8.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/typescript"
    ]
  },
  "eslintConfig": {
    "extends": "@deseteral/eslint-config/typescript"
  }
}
